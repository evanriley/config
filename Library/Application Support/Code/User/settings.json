        // {
        //     "before":["leader", "e", "f"],
        //     "commands": [
        //         "alive.loadFile",
        //     ]

        // },
        //Calva Binds
        {
            "before":["leader", "e", "j"],
            "commands": [
                "calva.jackIn",
            ]

        },
        {
            "before":["leader", "e", "d"],
            "commands": [
                "calva.disconnect",
            ]
        },
        {
            "before":["leader", "e", "f"],
            "commands": [
                "calva.loadFile",
            ]
        },
        {
            "before":["leader", "e", "F"],
            "commands": [
                "calva.loadNamespace",
            ]
        },
        {
            "before":["leader", "e", "e"],
            "commands": [
                "calva.evaluateCurrentTopLevelForm",
            ]
        },
        {
            "before":["leader", "e", "c", "e"],
            "commands": [
                "calva.evaluateSelection",
            ]
        },
        {
            "before":["leader", "e", "c", "f"],
            "commands": [
                "calva.evaluateEnclosingForm",
            ]
        },
        {
            "before":["leader", "e", "t", "d"],
            "commands": [
                "calva.debug.instrument",
            ]
        },
        {
            "before":["leader", "e", "i"],
            "commands": [
                "calva.interruptAllEvaluations",
            ]
        },
        {
            "before":["leader", "c", "q","c"],
            "commands": [
                "calva.evalCurrentFormInREPLWindow",
            ]
        },
    ],
    "tabnine.experimentalAutoImports": true,
    "workbench.iconTheme": "material-icon-theme",
    "gitlens.currentLine.enabled": false,
    "git.confirmSync": false,
    "security.workspace.trust.untrustedFiles": "open",
    "go.toolsManagement.autoUpdate": true,
    "vscode-neovim.neovimExecutablePaths.darwin": "/opt/homebrew/bin/nvim",
    "editor.accessibilitySupport": "off",
}
